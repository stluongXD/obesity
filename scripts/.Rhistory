library(dplyr)
library(ggplot2)
library(tidyr)
library(knitr)
obesity <- read.csv("../data/Obesity_GDP_PanelData.csv")
income_plot <- ggplot(obesity,
aes(x = Average.Income,
y = Adult.Obesity.100,
color = Year)) +
geom_point() +
xlab("Average Income") +
ylab("Percentage of Adult Obesity By State And Year") +
ggtitle("Income vs Obesity Rate")
